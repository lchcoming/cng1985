<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="trees" namespace="/"
		extends="struts-default">

		

		<action name="treetype" class="TypeAction" method="work">
			<result name="success" >
			/treetype/index.jsp
			</result>
		</action>
		<action name="treetypejson" class="TypeAction" method="tree">
			<result name="success" >
			/treetype/json.jsp
			</result>
		</action>
		<action name="treetypeone" class="TypeAction" method="makeOne">
			<result name="success" >
			/treetype/one.jsp
			</result>
		</action>
		<action name="treetypemore" class="TypeAction" method="makeMore">
			<result name="success" >
			/treetype/list.jsp
			</result>
		</action>
	</package>
</struts>
