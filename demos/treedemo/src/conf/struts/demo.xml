<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="cailiaoback" namespace="/"
		extends="struts-default">

		<action name="hi" class="HelloAction" method="hi">
			<result name="success">/demo.jsp</result>
		</action>
		<action name="tree" class="TreeAction" method="work">
			<result name="success">/jtree/tree.jsp</result>
		</action>
		<action name="datatree" class="DataTreeAction" method="work">
			<result name="success">/jtree/datatree.jsp</result>
			<result name="datasuccess">/jtree/filedatatree.jsp</result>
		</action>

		<action name="dtree" class="DtreeAction" method="work">
			<result name="success">/dtree/dtree.jsp</result>
		</action>
	</package>
</struts>
